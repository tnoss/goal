          }
        },
        "userDisplayName": {
          "type": "string"
        }
      }
    },
    "entity.UserBookingItem": {
      "type": "object",
      "properties": {
        "bookingDate": {
          "type": "string"
        },
        "bookingId": {
          "type": "string"
